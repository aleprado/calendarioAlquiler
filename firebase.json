{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      { "source": "/health", "function": { "functionId": "calendar-api", "region": "us-central1" } },
      { "source": "/properties/**", "function": { "functionId": "calendar-api", "region": "us-central1" } },
      { "source": "/public/**", "function": { "functionId": "calendar-api", "region": "us-central1" } },
      { "source": "**", "destination": "/index.html" }
    ]
  }
}
